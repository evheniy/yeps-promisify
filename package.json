{
  "name": "yeps-promisify",
  "version": "1.1.1",
  "description": "Recursive loop promise generator",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:**",
    "test:lint": "eslint index.js tests",
    "test:security": "nsp check",
    "test:code": "mocha tests --recursive",
    "test:coverage": "istanbul cover _mocha -- tests --recursive",
    "report": "cat ./coverage/lcov.info | coveralls",
    "clear": "rimraf coverage",
    "precommit": "npm run lint && npm test",
    "prepush": "npm run lint && npm test"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evheniy/yeps-promisify.git"
  },
  "keywords": [
    "yeps",
    "recursive",
    "promise",
    "generator",
    "async",
    "await"
  ],
  "author": "Evheniy Bystrov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evheniy/yeps-promisify/issues"
  },
  "homepage": "https://github.com/evheniy/yeps-promisify#readme",
  "files": [
    "index.js"
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^0.14.3",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "debug": "^3.1.0"
  }
}
